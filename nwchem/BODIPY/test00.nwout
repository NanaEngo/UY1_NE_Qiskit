end

driver
  clear
  maxiter 100
end

dft
  xc b3lyp
  iterations 500
  grid xfine
  disp vdw 4
end

cosmo
  solvent toluene
end

tddft
  nroots 5
  algorithm 1
  notriplet
  target 1
  targetsym a
  civecs
  grad
    root 1
  end

end
task tddft optimize
task tddft frequencies

tddft
  nroots 5
  algorithm 1
  nosinglet
  target 1
  targetsym a
  civecs
  grad
    root 1
  end

end
task tddft optimize
task tddft frequencies

# Calcul des propriétés électroniques
dplot
  vectors ascii
  limit 10
  filename orbitals
end
task dplot

